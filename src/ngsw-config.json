{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "employee-data",
            "urls": [
                "https://2f3299cb.ngrok.io/getAllCustProfile",
                "http://127.0.0.1:8080/manifest.json"
            ],
            "cacheConfig": {
                "strategy": "freshness",
                "maxsize": 200,
                "maxAge": "2h",
                "timeout": "10s"
            }
        }
    ]
}